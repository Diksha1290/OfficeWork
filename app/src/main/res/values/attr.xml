<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomEndlessView">
        <attr name="threshold" format="string" />
        <attr name="offset" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomTextView">
        <attr name="customtypeface" format="string" />
        <attr name="customtextsize" format="string" />
        <attr name="customwidth" format="string" />
        <attr name="customheight" format="string" />
        <attr name="ismandatory" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="CustomImageView">
        <attr name="height1" format="integer" />
        <attr name="width" format="integer" />
        <attr name="url" format="string" />
        <attr name="defaultImage" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconView">
        <attr name="theme_color" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="GaugeView">
        <attr name="innerCircleColor" format="color"/>
        <attr name="outerCircleColor" format="color"/>
        <attr name="showOuterShadow" format="boolean"/>
        <attr name="showOuterBorder" format="boolean"/>
        <attr name="showOuterRim" format="boolean"/>
        <attr name="showInnerRim" format="boolean"/>
        <attr name="showNeedle" format="boolean"/>
        <attr name="showScale" format="boolean"/>
        <attr name="showRanges" format="boolean"/>
        <attr name="showRangeText" format="boolean"/>
        <attr name="outerShadowWidth" format="float"/>
        <attr name="outerBorderWidth" format="float"/>
        <attr name="outerRimWidth" format="float"/>
        <attr name="innerRimWidth" format="float"/>
        <attr name="innerRimBorderWidth" format="float"/>
        <attr name="needleWidth" format="float"/>
        <attr name="needleHeight" format="float"/>
        <attr name="scalePosition" format="float"/>
        <attr name="scaleStartValu" format="float"/>
        <attr name="scaleEndValue" format="float"/>
        <attr name="scaleStartAngle" format="float"/>
        <attr name="scaleStartValue" format="float"/>
        <attr name="divisions" format="float"/>
        <attr name="subdivisions" format="float"/>
        <attr name="textShadowColor" format="color"/>
        <attr name="textValue" format="integer"/>
        <attr name="textUnit" format="integer"/>
        <attr name="textValueColor" format="color"/>
        <attr name="textUnitColor" format="color"/>
        <attr name="textValueSize" format="color"/>
        <attr name="textUnitSize" format="color"/>
        <attr name="rangeValues" format="reference"/>
        <attr name="rangeColors" format="reference"/>
    </declare-styleable>

    <declare-styleable name="cs_CircularSeekBar">
        <attr name="cs_circle_style">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="cs_progress" format="integer"/>
        <attr name="cs_max" format="integer"/>
        <attr name="cs_negative_enabled" format="boolean"/>
        <attr name="cs_move_outside_circle" format="boolean"/>
        <attr name="cs_maintain_equal_circle" format="boolean"/>
        <attr name="cs_use_custom_radii" format="boolean"/>
        <attr name="cs_lock_enabled" format="boolean"/>
        <attr name="cs_circle_x_radius" format="dimension" />
        <attr name="cs_circle_y_radius" format="dimension" />
        <attr name="cs_circle_stroke_width" format="dimension" />
        <attr name="cs_disable_pointer" format="boolean"/>
        <attr name="cs_pointer_stroke_width" format="dimension" />
        <attr name="cs_pointer_halo_width" format="dimension" />
        <attr name="cs_pointer_halo_border_width" format="dimension"/>
        <attr name="cs_circle_fill" format="color"/>
        <attr name="cs_circle_color" format="color"/>
        <attr name="cs_circle_progress_color" format="color"/>
        <attr name="cs_pointer_color" format="color"/>
        <attr name="cs_pointer_halo_color" format="color"/>
        <attr name="cs_pointer_halo_color_ontouch" format="color"/>
        <attr name="cs_pointer_alpha_ontouch" format="integer"/>
        <attr name="cs_pointer_angle" format="float"/>
        <attr name="cs_start_angle" format="float"/>
        <attr name="cs_end_angle" format="float"/>
        <attr name="cs_disable_progress_glow" format="boolean"/>
        <attr name="cs_hide_progress_when_empty" format="boolean"/>
    </declare-styleable>

        <declare-styleable name="TouchImageView">
            <attr name="zoom_enabled" format="boolean" />
        </declare-styleable>


</resources>